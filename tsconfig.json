{
	"compileOnSave": false,
	"compilerOptions": {
		"strict": true,
		"noEmit": true,
		"declaration": true,
		"moduleResolution": "node",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"lib": [
			"dom",
			"es5",
			"es2015",
			"es2015.iterable"
		]
	},
	"include": [
		"src/*.ts",
		"test/*.ts"
	]
}
